<!--ヘッダー-->
<header>
<!--PC用（901px以上端末）メニュー-->
  <%# <nav id="main-menubar"> %>
  <nav id="main-menubar">
    <ul class="menu">
      <li class='list-menu'>
        <%= link_to 'Search', photos_search_path, class: "serch" %>
      <% if user_signed_in? %>
        <%= link_to 'My Page', user_path(current_user.id), class: "mypage" %>
        <%= link_to 'Logout', destroy_user_session_path, method: :delete, class: "logout" %>
        <%= link_to 'Post', new_photo_path, class: "post" %>
      <% else %>
        <%= link_to 'Signin',new_user_registration_path, class: "signin" %>
        <%= link_to 'Login', new_user_session_path , method: :post, class: "login" %>
      <% end %>
      </li>
    </ul>
    <ul class="icon">
      <li class='list-icon'>
        <%= link_to @nickname , user_path(current_user.id), class: :greeting__link %>
        <%= link_to image_tag("icon_facebook.png", class:"icon"),"/" %>
        <%= link_to image_tag("icon_twitter.png", class:"icon"),"/" %>
        <%= link_to image_tag("icon_instagram.png", class:"icon"),"/" %>
        <%= link_to image_tag("icon_youtube.png", class:"icon"),"/" %>
      </li>
    </ul>
  </nav>
</header>
<!--ヘッダー-->
